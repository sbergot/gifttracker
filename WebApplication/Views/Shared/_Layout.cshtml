<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"]</title>
  <link rel="stylesheet" href="~/css/spectre.min.css" />
  <link rel="stylesheet" href="~/css/spectre-icons.min.css" />
  <link rel="stylesheet" href="~/css/spectre-exp.min.css" />
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
  <header class="navbar container grid-md">
    <section class="navbar-brand mr-2">
      <a class="brand-icon" asp-page="/Index"></a>
      <span class="brand-title">
        giftTracker
      </span>
    </section>
  </header>


  <div class="container grid-md">
    @RenderBody()
  </div>

  <footer class="container grid-md">
  </footer>

  <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.3.js"></script>
  @RenderSection("scripts", required: false)
</body>
</html>
